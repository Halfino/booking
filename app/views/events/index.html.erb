<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Datetimefrom</th>
    <th>Datetimeto</th>
    <th>Image</th>
    <th>Descrioptionenglish</th>
    <th>Descriptionczech</th>
    <th>Forumlink</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @events.each do |event| %>
    <tr>
      <td><%= event.name %></td>
      <td><%= event.dateTimeFrom %></td>
      <td><%= event.dateTimeTo %></td>
      <td><%= event.image %></td>
      <td><%= event.descrioptionEnglish %></td>
      <td><%= event.descriptionCzech %></td>
      <td><%= event.forumLink %></td>
      <td><%= link_to 'Show', event %></td>
      <td><%= link_to 'Edit', edit_event_path(event) %></td>
      <td><%= link_to 'Destroy', event, method: :delete, data: {confirm: 'Are you sure?'} %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
